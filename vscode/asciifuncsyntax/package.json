{
	"name": "asciifuncsyntax",
	"displayName": "asciifunc Syntax Highlighter",
	"description": "Syntax highlighter for the asciifunc Esolang",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "asciifunc",
				"aliases": [
					"asciifunc",
					"asciifunc"
				],
				"extensions": [
					".ascf"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "asciifunc",
				"scopeName": "source.ascf",
				"path": "./syntaxes/asciifunc.tmLanguage.json"
			}
		]
	}
}